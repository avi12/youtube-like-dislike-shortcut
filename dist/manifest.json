{
  "name": "YouTube Like-Dislike Shortcut",
  "short_name": "YT Like Btn",
  "description": "Shift+Plus or Number Plus to like, Shift+Minus or Numpad Minus to dislike. Can't get any simpler.",
  "version": "1.1.0",
  "author": "avi12",
  "developer": {
    "name": "avi12",
    "url": "https://avi12.com/youtube-like-dislike-shortcut"
  },
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["build/content-script-youtube-rate-trigger.js"]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "19": "images/icon-19.png",
    "24": "images/icon-24.png",
    "32": "images/icon-32.png",
    "38": "images/icon-38.png",
    "48": "images/icon-48.png",
    "64": "images/icon-64.png",
    "96": "images/icon-96.png",
    "128": "images/icon-128.png"
  },
  "offline_enabled": true,
  "applications": {
    "gecko": {
      "id": "youtube-like-dislike-shortcut@avi12.com"
    }
  }
}
