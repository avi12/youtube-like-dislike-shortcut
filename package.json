{
  "name": "youtube-like-dislike-shortcut",
  "version": "1.7.0",
  "description": "Like videos with Shift+Plus or Numpad Plus, dislike with Shift+Minus or Numpad Minus.",
  "repository": "https://github.com/avi12/youtube-like-dislike-shortcut",
  "license": "GPL-3.0-or-later",
  "author": "avi12 <avi6106@gmail.com>",
  "homepage": "https://avi12.com/youtube-like-dislike-shortcut",
  "bugs": {
    "email": "avi6106@gmail.com"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "package": "web-ext build --filename youtube-like-dislike-shortcut-{version}.zip --source-dir dist",
    "package:self": "zip-self -o web-ext-artifacts/youtube-like-dislike-shortcut-{version}-source.zip",
    "build:firefox": "pnpm package:self",
    "build-pack": "pnpm build && pnpm package && pnpm webext-incompat-fix",
    "build-pack:all": "pnpm build-pack && pnpm build:firefox",
    "webext-incompat-fix": "webext-store-incompat-fixer -i web-ext-artifacts/youtube-like-dislike-shortcut-{version}.zip -s chrome,edge,firefox,opera",
    "set-version": "set-json-version -f package.json dist/manifest.json -v",
    "run-browser": "web-ext run --start-url https://www.youtube.com/watch?v=aiSla-5xq3w --source-dir dist --verbose --target",
    "run-firefox": "pnpm run-browser firefox-desktop",
    "run-chromium": "pnpm run-browser chromium",
    "run-edge-windows": "pnpm run-chromium --chromium-binary \"%ProgramFiles(x86)%\\Microsoft\\Edge\\Application\\msedge.exe\"",
    "run-opera-windows": "pnpm run-chromium --chromium-binary \"%LocalAppDAta%\\Programs\\Opera\\opera.exe\""
  },
  "devDependencies": {
    "@mdi/js": "^7.2.96",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "@sveltejs/eslint-config": "github:sveltejs/eslint-config",
    "@tsconfig/svelte": "^5.0.2",
    "@types/chrome": "^0.0.246",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "sass": "^1.68.0",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "rollup": "^3.29.2",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-svelte": "^7.1.6",
    "set-json-version": "^2.1.5",
    "svelte": "^4.2.1",
    "svelte-materialify": "^0.3.11",
    "svelte-preprocess": "^5.0.4",
    "typescript": "^5.2.2",
    "tslib": "^2.6.2",
    "web-ext": "^7.7.0",
    "webext-store-incompat-fixer": "0.4.2",
    "zip-self": "^0.5.9"
  }
}
